  "keywords": [
    "i18n",
    "internationalization",
    "localization",
    "translation",
    "management",
    "toolkit",
    "javascript",
    "typescript",
    "react",
    "vue",
    "angular",
    "cli",
    "automation",
    "validation",
    "analysis",
    "enterprise",
    "debugging",
    "reporting",
    "path-resolution",
    "directory-configuration"
  ],
  "homepage": "https://github.com/vladnoskv/i18ntk#readme",
  "bugs": {
    "url": "https://github.com/vladnoskv/i18ntk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladnoskv/i18ntk.git"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/vladnoskv"
  },
  "license": "MIT",
  "author": "Vladimir Noskov <vladno@hotmail.co.uk> (https://github.com/vladnoskv)",
  "type": "commonjs",
  "main": "main/i18ntk-manage.js",
  "bin": {
    "i18ntk": "main/i18ntk-manage.js",
    "i18ntk-manage": "main/i18ntk-manage.js",
    "i18ntk-init": "main/i18ntk-init.js",
    "i18ntk-analyze": "main/i18ntk-analyze.js",
    "i18ntk-validate": "main/i18ntk-validate.js",
    "i18ntk-usage": "main/i18ntk-usage.js",
    "i18ntk-complete": "main/i18ntk-complete.js",
    "i18ntk-sizing": "main/i18ntk-sizing.js",
    "i18ntk-summary": "main/i18ntk-summary.js",
    "i18ntk-autorun": "main/i18ntk-autorun.js"
  },
  "directories": {
    "doc": "docs"
  },
  "files": [
    "*.js",
    "main/",
    "utils/",
    "scripts/",
    "settings/",
    "ui-locales/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node main/i18ntk-manage.js",
    "i18ntk": "node main/i18ntk-manage.js",
    "i18ntk:init": "node main/i18ntk-init.js",
    "i18ntk:analyze": "node main/i18ntk-analyze.js",
    "i18ntk:validate": "node main/i18ntk-validate.js",
    "i18ntk:usage": "node main/i18ntk-usage.js",
    "i18ntk:complete": "node main/i18ntk-complete.js",
    "i18ntk:sizing": "node main/i18ntk-sizing.js",
    "i18ntk:sizing:detailed": "node main/i18ntk-sizing.js --detailed",
    "i18ntk:debug": "node main/i18ntk-manage.js --command=debug",
    "i18ntk:settings": "node settings/settings-cli.js",
    "settings": "node settings/settings-cli.js",
    "i18ntk:summary": "node main/i18ntk-summary.js",
    "i18ntk:manage": "node main/i18ntk-manage.js",
    "i18n:autorun": "node main/i18ntk-autorun.js",
    "i18ntk:autorun": "node main/i18ntk-autorun.js",
    "workflow": "node main/i18ntk-autorun.js",
    "init": "node main/i18ntk-init.js",
    "analyze": "node main/i18ntk-analyze.js",
    "validate": "node main/i18ntk-validate.js",
    "usage": "node main/i18ntk-usage.js",
    "complete": "node main/i18ntk-complete.js",
    "sizing": "node main/i18ntk-sizing.js",
    "summary": "node main/i18ntk-summary.js",
    "full-coverage": "npm run complete && npm run analyze",
    "verify-package": "node scripts/verify-package.js",
    "prepublishOnly": "npm run verify-package && node scripts/prepublish.js",
    "test": "node scripts/test-runner.js",
    "test:all": "node scripts/test-runner.js",
    "test:quick": "node scripts/test-runner.js --quick",
    "test:admin": "node scripts/test-runner.js --admin-pin",
    "test:comprehensive": "node scripts/test-runner.js --comprehensive",
    "test:pin": "node dev/tests/test-admin-pin.js",
    "test:edge-cases": "node dev/tests/test-comprehensive.js",
    "test:old": "node utils/test-complete-system.js",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:check": "node utils/security-check.js",
    "security:config": "node utils/security-config.js",
    "benchmark": "node benchmarks/run-benchmarks.js",
    "benchmark:ci": "node benchmarks/run-benchmarks.js --ci-mode",
    "benchmark:baseline": "node benchmarks/run-benchmarks.js && node -e \"const fs = require('fs'); const path = require('path'); const date = new Date().toISOString().split('T')[0]; const source = path.join('benchmarks', 'results', 'benchmark-' + date + '.json'); const target = path.join('benchmarks', 'baseline.json'); if (fs.existsSync(source)) { fs.copyFileSync(source, target); console.log('✅ Baseline updated successfully'); } else { console.error('❌ Source file not found:', source); process.exit(1); }\"",
    "languages:select": "node settings/settings-cli.js",
    "languages:list": "node settings/settings-cli.js --list-languages",
    "languages:status": "node settings/settings-cli.js --language-status",
    "languages:restore": "echo 'Language restoration - use settings CLI for manual configuration'"
  },
  "dependencies": {
    "i18ntk": "^1.5.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "preferGlobal": true,
